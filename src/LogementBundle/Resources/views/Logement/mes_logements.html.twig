        document.getElementById('t'+id).style.border = "thick solid gray";
        document.getElementById('p'+id).style.border = "thick solid gray";
        document.getElementById('d'+id).style.border = "thick solid gray";


        document.getElementById('edit'+id).style.display = "none";
        document.getElementById('save'+id).style.display = "block";





    }

