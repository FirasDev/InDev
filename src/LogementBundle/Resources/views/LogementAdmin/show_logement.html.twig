<div class="content" style="
    padding-left: 150px;
    padding-top: 90px;
height: 100%;
">


    <center><table class="responstable" style="width: 80%">

        <tr>
            <th>Image</th>
            <th>Titre</th>
            <th>Description</th>
            <th>Prix</th>
            <th>Disponibilité</th>
            <th>Approuver</th>

        </tr>

            {% for log in logements %}
        <tr>
            <td><img src="{{ asset('images/') }}{{ log.url }}" alt=""  style="max-width: 80%;max-height: 80%"></td>
            <td>{{ log.titre }}</td>

            <td>{{ log.description|slice(0, 50) ~ '...' }}</td>

            <td>{{ log.prix }}</td>
            {% if (log.isdisponible == true) %}
            <td>Disponible</td>
            {% else %}
                <td>Non Disponible</td>
                {% endif %}

            <td>
                <form method="post" >
                    {% if (log.isapproved == true) %}
                <button type="submit" name="approuve" class="btn-primary" value="{{ log.id }}" disabled>Approuved</button>
                {% else %}
                    <button type="submit" name="approuve" class="btn-primary" value="{{ log.id }}" >Approuver</button>
                    <button type="submit" name="desapprouve" class="btn-danger" value="{{ log.id }}" >Désapprouver</button>

                    {% endif %}


</form>
                    </td>

        </tr>
            {% endfor %}

    </table>
    </center>


