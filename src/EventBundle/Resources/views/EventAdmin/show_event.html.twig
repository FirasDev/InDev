                <button type="submit" name="approuve" class="btn-danger" value="{{ ev.idEvent }}" disabled>Approuver</button>
                {% else %}
                    <button type="submit" name="approuve" class="btn-primary" value="{{ ev.idEvent }}" >Approuver</button>
                    {% endif %}


</form>
                    </td>
        </tr>
            {% endfor %}

    </table>
