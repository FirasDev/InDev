{#{% extends 'base.html.twig' %}#}

{#{% block body %}#}


    {#<h1>Experience</h1>#}

    {#<table>#}
        {#<tbody>#}
            {#<tr>#}
                {#<th>Titreexp</th>#}
                {#<td>{{ experience.titreExp }}</td>#}
            {#</tr>#}
            {#<tr>#}
                {#<th>Descexp</th>#}
                {#<td>{{ experience.descExp }}</td>#}
            {#</tr>#}
            {#<tr>#}
                {#<th>Dateexp</th>#}
                {#<td>{% if experience.dateExp %}{{ experience.dateExp|date('Y-m-d') }}{% endif %}</td>#}
            {#</tr>#}
            {#<tr>#}
                {#<th>Evalexp</th>#}
                {#<td>{{ experience.evalExp }}</td>#}
            {#</tr>#}
            {#<tr>#}
                {#<th>Image</th>#}
                {#<td><img style="width: 220px;" src="{{ asset("assets/images/experience/"~ experience.image ) }}" border="0" alt="No image found" class="pb-img"></td>#}
            {#</tr>#}
            {#<tr>#}
                {#<th>Season</th>#}
                {#<td>{{ experience.season }}</td>#}
            {#</tr>#}
        {#</tbody>#}
    {#</table>#}

    {#<ul>#}
        {#<li>#}
            {#<a href="{{ path('experience_index') }}">Back to the list</a>#}
        {#</li>#}
        {#<li>#}
            {#<a href="{{ path('experience_edit', { 'idExperience': experience.idExperience }) }}">Edit</a>#}
        {#</li>#}

    {#</ul>#}
    {% stylesheets filter="scssphp" output="css/app.css"
    "assets/css/comment_style.css"
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    {#<section>#}
    {#<div id="noteFeed">#}
        {#<div id="noteCommentBox">#}
            {#<div class="feed-item">#}
                {#<textarea placeholder="Add a note here..." wrap="off" id="myNote"></textarea>#}
            {#</div>#}
            {#<button id="send">Send</button>#}
        {#</div>#}
    {#</div>#}
    {#<div id = "comments_sec">#}
        {#{% include 'ExperienceBundle:experience:comment.html.twig' %}#}
    {#</div>#}
    {#</section>#}





{#<script language="JavaScript">#}
    {#setInterval(function(){#}
        {#getComments();#}
    {#},3000);#}

    {#function getComments(){#}
        {#$.ajax({#}
            {#type: "get",#}
            {#url: "{{ path('experience_show', { 'idExperience': experience.idExperience }) }}",#}
            {#success: function (data) {#}
                {#$('#comments_sec').html(data.html);#}
            {#}#}
        {#});#}
    {#}#}

    {#$('#send').on('click', function (event) {#}
        {#event.preventDefault();#}
        {#$.ajax({#}
            {#type: "post",#}
            {#url: "{{ path('add_comment') }}",#}
            {#data : {comment : $('#myNote').val(), id_exp: {{ experience.idExperience}} },#}
            {#success: function (data) {#}
                {#getComments();#}
                {#$('#myNote').val("")#}
            {#}#}
        {#});#}
    {#});#}
{#</script>#}

{#{% endblock %}#}

